<!DOCTYPE html>
<html>
<head>
    <title>Suno-Vercel Pairs</title>
</head>
<body>
    <h1>Suno-Vercel Pairs</h1>
    <a href="/ui/add">Add New Pair</a>
    <table border="1">
        <tr>
            <th>ID</th>
            <th>API Endpoint</th>
            <th>Email</th>
            <th>Status</th>
            <th>Actions</th>
        </tr>
        <% pairs.forEach(function(pair) { %>
            <tr>
                <td><%= pair.id %></td>
                <td><%= pair.api_endpoint_url %></td>
                <td><%= pair.email %></td>
                <td><%= pair.status %></td>
                <td>
                    <a href="/ui/edit/<%= pair.id %>">Edit</a>
                    <a href="/ui/delete/<%= pair.id %>" onclick="return confirm('Are you sure?')">Delete</a>
                </td>
            </tr>
        <% }); %>
    </table>
</body>
</html>
